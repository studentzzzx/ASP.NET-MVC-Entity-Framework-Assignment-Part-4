@model IEnumerable<Insurance.Models.Insuree>

@{
    ViewBag.Title = "Admin";
}

<h2>Admin - All Quotes</h2>

<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email Address</th>
        <th>Quote</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>@item.FirstName</td>
        <td>@item.LastName</td>
        <td>@item.EmailAddress</td>
        <td>@item.Quote.ToString("C")</td>
    </tr>
}
</table>
